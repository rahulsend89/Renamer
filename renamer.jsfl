function check () {	my_doc=fl.getDocumentDOM();	if (my_doc.selection.length != 1) {		return "tooMany";	}	else if (my_doc.selection[0].elementType="text") {		return "text";	}	else if (my_doc.selection[0].getPersistentData("waved") == "true") {		return "edit";	}	else {		return "text";	}}function test (pre,post,id,includeper) {	my_doc=fl.getDocumentDOM();	if (id == 1) {		fl.trace ("Instance name renamer");		var my_frame=my_doc.getTimeline().layers[0].frames[0];		var elems=my_frame.elements;		for (var i=0; i < elems.length; i++) {			my_doc.selectNone ();			my_doc.selection=[elems[i]];			fl.trace (i + " : ");			//letterMc = my_doc.convertToSymbol("movie clip","letter" + i,"center");			//my_doc.selection[0].colorAlphaPercent = 0;			my_doc.selection[0].name=pre + "" + i + "" + post;		}		return elems.length;	}	else if (id == 2) {		fl.trace ("Library renamer");		var lib_items=my_doc.library.getSelectedItems();		if (lib_items.length == 0) {			fl.trace ("Please select at least one item in the library.");		}		else {			for (var i=0; i < lib_items.length; i++) {				var item=lib_items[i];				if (includeper == "true") {					item.name=pre + item.name + post;				}				else {					item.name=pre + "" + i + "" + post;				}				fl.trace (i + " : ");			}		}	}}